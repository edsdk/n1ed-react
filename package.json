{
  "name": "@edsdk/n1ed-react",
  "version": "1.0.15",
  "description": "React integration for N1ED editor",
  "repository": {
    "url": "https://github.com/edsdk/n1ed-react"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE.txt"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/es2015/index.js",
  "scripts": {
    "lint": "tslint 'src/**/*.ts?(x)'",
    "clean": "rimraf lib",
    "build": "npm run clean && tsc -p ./tsconfig.es2015.json && tsc -p ./tsconfig.cjs.json",
    "watch": "tsc -w -p ./tsconfig.es2015.json",
    "prepare": "npm run build"
  },
  "keywords": [
    "tinymce",
    "ckeditor",
    "n1ed",
    "react",
    "editor",
    "html",
    "wysiwyg",
    "visual",
    "page",
    "builder"
  ],
  "author": "https://n1ed.com",
  "license": "LGPL v3",
  "dependencies": {
    "@tinymce/tinymce-react": "^3.0.0 || ^4.0.0",
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@types/node": "^10.17.60",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^16.14.34",
    "@types/react-dom": "^16.9.17",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.3.0",
    "prop-types": "^15.8.1",
    "raf": "^3.4.1",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "typescript": "^3.9.10"
  }
}
